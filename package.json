{
  "name": "pogo-web-backend",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "generate_mainseries_moveset_data": "node --loader ts-node/esm --experimental-specifier-resolution=node updateData/makeMainSeriesMoveset.ts",
    "generate_pokemonforformats_data": "node --loader ts-node/esm --experimental-specifier-resolution=node  updateData/preparePokemonForFormatsData.ts",
    "start_matchhandling": "node --loader ts-node/esm --experimental-specifier-resolution=node -r dotenv/config matchhandling_server.ts",
    "start_rest": "node --loader ts-node/esm --experimental-specifier-resolution=node -r dotenv/config api/rest_server.ts",
    "update_data": "node --loader ts-node/esm --experimental-specifier-resolution=node updateData/addUnreleasedTags.ts && node --loader ts-node/esm --experimental-specifier-resolution=node updateData/makeMainSeriesMoveset.ts && node --loader ts-node/esm --experimental-specifier-resolution=node updateData/preparePokemonForFormatsData.ts",
    "dev": "npm run start_matchhandling && npm run start_rest",
    "dev_concurrently": "concurrently \"npm run start_matchhandling\" \"npm run start_rest\"",
    "start": "npm run runbuild",
    "test": "echo \"Error: no test specified\" && exit 1",
    "postinstall": "tsc",
    "runbuild": "concurrently \"node --experimental-specifier-resolution=node dist/matchhandling_server.js\" \"node --experimental-specifier-resolution=node dist/api/rest_server.js\"",
    "secrets": "npm run secrets:logout && cross-env-shell BW_SESSION=`bw login projectgrookey@gmail.com --raw` \"npm run secrets:sync\"",
    "secrets:logout": "(bw logout || exit 0)",
    "secrets:login": "bw login projectgrookey@gmail.com",
    "secrets:sync": "bw sync && bw get item 377ad645-94d0-4e13-8e24-aca7013a9e8c | fx .notes > \".env\""
  },
  "engines": {
    "node": "14.13.0"
  },
  "keywords": [],
  "author": "",
  "license": "MIT",
  "type": "module",
  "types": "./types/index.ts",
  "dependencies": {
    "@types/bad-words": "^3.0.0",
    "@types/cors": "^2.8.9",
    "@types/deep-equal": "^1.0.1",
    "@types/express": "^4.17.9",
    "@types/ioredis": "^4.26.0",
    "@types/jsonwebtoken": "^8.5.0",
    "@types/node": "^14.14.12",
    "@types/uuid": "^8.3.0",
    "@types/ws": "^7.4.0",
    "bad-words": "^3.0.4",
    "concurrently": "^6.1.0",
    "cors": "^2.8.5",
    "deep-equal": "^2.0.5",
    "dotenv": "^8.2.0",
    "express": "^4.16.3",
    "firebase-admin": "^9.4.2",
    "ioredis": "^4.27.1",
    "jsonwebtoken": "^8.5.1",
    "ts-node": "^9.1.1",
    "typescript": "^4.1.3",
    "uuid": "^8.3.2",
    "ws": "^7.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DeveloperKhan/pogo-web-backend.git"
  },
  "devDependencies": {
    "@bitwarden/cli": "^1.15.1",
    "cross-env": "^7.0.3",
    "fx": "^20.0.2"
  }
}
